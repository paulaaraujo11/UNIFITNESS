var Mediator = (function(){
  var channels = {};

  function subscribe(channel, fn) {
    if (channels[channel] === undefined) {
      channels[channel] = [];
    }
    channels[channel].push({ context: this, callback: fn });
    return this;
  }

  function publish(channel) {
    var args;

    if (channels[channel] === undefined) {
      return false;
    }

    args = Array.prototype.slice.call(arguments, 1);
    for(var i = 0, l = channels[channel].length; i < l; i++) {
      var subscription = channels[channel][i];
      subscription.callback.apply(subscription.context, args);
    }
    return this;
  }

  return {
    subscribe: subscribe,
    publish: publish
  };
}());

function Timeout(time) {
  var maxTime = time || 5000,
      timeoutID = null;

  if (!(this instanceof Timeout)) {
    return new Timeout(maxTime);
  }

  function set(callback) {
    unset();
    timeoutID = setTimeout(function() {
      callback.call();
    }, maxTime);
  }

  function unset() {
    if (timeoutID !== null) {
      window.clearTimeout(timeoutID);
    }
  }

  return {
    set: set,
    unset: unset
  };
}

function pluralize(qtty, singular, plural) {
  if (qtty == 1) return qtty + " " + singular
  return qtty + " " + plural
}

function number_to_currency(n, decPlaces, thouSeparator, decSeparator) {
  n = parseFloat(n);
  decPlaces = isNaN(decPlaces = Math.abs(decPlaces)) ? 2 : decPlaces,
  decSeparator = decSeparator == undefined ? "," : decSeparator,
  thouSeparator = thouSeparator == undefined ? "." : thouSeparator,
  sign = n < 0 ? "-" : "",
  i = parseInt(n = Math.abs(+n || 0).toFixed(decPlaces)) + "",
  j = (j = i.length) > 3 ? j % 3 : 0;
  return sign + (j ? i.substr(0, j) + thouSeparator : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thouSeparator) + (decPlaces ? decSeparator + Math.abs(n - i).toFixed(decPlaces).slice(2) : "");
}

function numberToCurrency(n, decPlaces, thouSeparator, decSeparator) {
  return number_to_currency(n, decPlaces, thouSeparator, decSeparator);
}

function currency_to_number( c ) {
  return parseFloat(c.replace(",", ".")).toFixed(2);
}

