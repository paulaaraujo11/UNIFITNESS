function pluralize(qtty, singular, plural) {
  if (qtty == 1) return qtty + " " + singular
  return qtty + " " + plural
}

$(document).ready(function(){
  if(!String.prototype.trim) {
    String.prototype.trim = function() {
      return this.replace(/^\s+|\s+$/g, '');
    }
  }

  Array.prototype.each_slice = function (size, callback){
    for (var i = 0, l = this.length; i < l; i += size) callback.call(this, this.slice(i, i + size));
  };

  if (!Array.prototype.indexOf) {
    Array.prototype.indexOf = function(elt /*, from*/) {
      var len = this.length >>> 0;
      var from = Number(arguments[1]) || 0;
      from = (from < 0) ? Math.ceil(from) : Math.floor(from);
      if (from < 0)
        from += len;

      for (; from < len; from++) {
        if (from in this && this[from] === elt)
          return from;
      }
      return -1;
    };
  }

  if (!Array.prototype.filter) {
    Array.prototype.filter= function(filter, that /*opt*/) {
      var other= [], v;
      for (var i=0, n= this.length; i<n; i++)
        if (i in this && filter.call(that, v= this[i], i, this))
        other.push(v);
      return other;
    };
  }

  Array.prototype.diff = function(a) {
    return this.filter(function(i) {return !(a.indexOf(i) > -1);});
  };

  Array.prototype.getUnique = function(){
    var u = {}, a = [];
    for(var i = 0, l = this.length; i < l; ++i){
      if(u.hasOwnProperty(this[i])) {
        continue;
      }
      a.push(this[i]);
      u[this[i]] = 1;
    }
    return a;
  };

  if( typeof $.datepicker != "undefined" ) {
    $.datepicker.setDefaults($.datepicker.regional["pt-BR"]);
    $('.datepicker').focus(function(){ $(this).datepicker(); });
  }

  if ($.fn.numeric) {
    $("input[type='number'], .number").numeric();
  }

  if ($.mask) {
    $.mask.masks.br_phone = {mask: '999999999'};
    $.mask.masks.br_new_phone = {mask: '(99) 9999-99999'};
    $.mask.masks.ddd = {mask: '99'};
    $.mask.masks.datetime = {mask: '39/19/9999 29:69:69'};
    $.mask.masks.integer = {mask: '999999999999'};
    $.mask.masks.decimal = { mask : '99,999999999999', type : 'reverse', defaultValue: '000' };
    $.mask.masks.cep = {mask : '99999999'};
    $.mask.masks.cvv = {mask: '9999'}
    $.mask.masks.two_integer = {mask: '99'}
    $('input:text').setMask();

    $("input[alt='br_new_phone']").focusout(function(evt){
      var phone, element;
      element = $(this);
      if( element.val().replace(/\D/g, '').length > 10 ) {
        element.setMask("(99) 99999-9999");
      } else {
        element.setMask("(99) 9999-9999");
      }
    });
  }

  // Placeholder para utilizar no Adena, basta colocar o class .placeholder_text e no campo
  // title o valor que ser utilizado como placeholder
  $(".placeholder_text").blur(function(){
    if( $(this).val() === '') {
      $(this).val($(this).attr("title"));
      $(this).css("color", "#CCC");
    } else {
      if ( $(this).val() == $(this).attr("title") )
        $(this).css("color", "#CCC");
      else
        $(this).css("color", "#000");
    }
  }).focus(function(){
    $(this).css("color", "#000");
    if( $(this).val() == $(this).attr("title")) $(this).val("");
  });
  $(".placeholder_text").trigger("blur");
});

