  $(selector).empty();
  if(data.error != undefined){
    $(selector).fadeIn();
    $(selector).append('<small><span class="bold-text">'+data.error+'</span></small>');
  } else {
    if ($.isEmptyObject(data)) {
      $(selector).fadeIn();
      $(selector).html(no_shipping_message);
    } else {
      error = '';
      shipping_valido = false;
      $.each(data, function(key, value){
        if($.isEmptyObject(value.error) && !(isNaN(value.price)) && value.price != null) {
          shipping_valido = true;
          $(selector).fadeIn();
          var shipping_value = (  value.promo_price != undefined ) ? value.promo_price : value.price
          result_radio = '<input type="radio" value="" name="shipping_service_values" class="shipping_values_radio" onclick="update_total_cart(' + shipping_value + ', \''+ key +'\')" />';
          if(show_radio == false){
            result_radio = "";
          }

          var shipping_service_label = "R$ " + number_to_currency(value.price);
          if( value.promo_price != null ){
            if ( value.promo_price == 0 ){
              shipping_service_label = "GRTIS";
            } else {
              shipping_service_label = "R$ " + number_to_currency(value.promo_price);;
            }
            $(selector).append(result_radio+'<b>'+key+':</b>  <small class="oldShippingValue">R$ '+number_to_currency(value.price)+'</small> '+shipping_service_label+" <span>(" + value.delivery_time + " dias teis)</span><br/>");
          } else {
            if ( value.price == 0 ){
              shipping_service_label = "GRTIS";
            }
            $(selector).append(result_radio+'<b>'+key+':</b>  '+shipping_service_label+" <span>(" + value.delivery_time + " dias teis)</span><br/>");
          }

        } else {
          $(selector).fadeIn();
          result_radio = "";
          if(show_radio == true){
            result_radio = '<input type="radio" value="" name="shipping_service_values" class="shipping_values_radio" disabled="disabled"/>';
          }
          $(selector).append(result_radio+'<b>'+key+'</b> <span class="price">'+value.error+'</span><br/>')
        }
