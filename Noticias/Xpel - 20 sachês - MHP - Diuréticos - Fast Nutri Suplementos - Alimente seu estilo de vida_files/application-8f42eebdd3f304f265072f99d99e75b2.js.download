(function($, Tpl){

  $.fn.smartSearch = function( options ){
    var cachedTokens = new Object()

    ,currentElement = null

    ,defaultOptions = {
      remoteUrl: "",
      parentBox: null,
      type: "GET",
      target: "body"
    }
    ,template = $("#partial-smart-search").html()

    ,isCached = function( token ){
      if( token in cachedTokens )
        return true;
      return false;
    }

    ,setCache = function( token, value ){
      cachedTokens[token] = value;
    }
    ,getCache = function( token ){
      return cachedTokens[token];
    }

    ,fetch = function( token ){
      $.ajax({
      type: options.type,
      url:  options.remoteUrl,
      dataType: "json",
      data: {q: token}
      }).done(function(data, textStatus, jqXHR){
        data.is_products = data.products.length > 0
        data.is_suggestions = data.suggestions.length > 0
        setCache( token, data );
        open( token );
      });
    }
    ,getProperties = function(){
      var properties    = $(currentElement).offset();
      properties.top    += $(currentElement).outerHeight();
      if (options.parentBox === null) {
        properties.width = $(currentElement).innerWidth();
      } else {
        properties.width = $(currentElement).parent(options.parentBox).innerWidth();
      }
      return properties;
    }

    ,open = function( token ){
      var view = getCache( token );
      if (view !== null) {
        view.properties = getProperties();
      }
      close();
      if(view !== null && view.suggestions.length + view.products.length > 0)
        $( options.target ).append( Tpl.render( template, view ) );
    }

    ,close = function(){
      $( "#popup-smart-search" ).remove();
    }

    ,delay = (function(){
      var timer = 0;
      return function(callback, ms){
        clearTimeout(timer);
        timer = setTimeout(callback, ms);
      };
    })();

    options = $.extend( defaultOptions, options );

    $(document).on("click", function(evt){
      close();
    }).on("click", "li[data-smart_search_remote_url]", function(evt){
      window.location = $(this).data("smart_search_remote_url");
    });

    $(window).scroll(function() {
      close();
    });

    return this.each(function(idx){
      var ident = "smart-search-input-"+ idx;
      $( this ).addClass( ident );

      $(document).on("keyup", "."+ident, function(evt){
        currentElement = this;

        delay(function(){
          var currentToken = $(currentElement).val();

          if( currentToken == "" ){
            close();
          }else if( isCached( currentToken ) ){
            open( currentToken );
          }else{
            setCache( currentToken, null );
            fetch( currentToken );
          }
        }, 300);
      });
    });
  };

})(jQuery, Mustache);

