(function(jQuery, Handlebars) {
  jQuery.fn.advancedCart = function(options) {
    var self = this,
        cart,
        template,
        isOpenedTimeout,
        messageTimeout,
        settings = {
          template: '#ad-advanced-cart-template',           // Identificador do template Handlesbar
          containerId: '.ad-advanced-cart-container',       // Identificador do cart
          headerId: '.ad-advanced-cart-header',             // Identificador do cart fechado
          itemsId: '.ad-advanced-cart-items',               // Identificador para o box de itens
          itemId: '.ad-advanced-cart-item',                 // Identificador de cada item do cart (onde deve ficar o data-key)
          removeId: '.ad-advanced-cart-remove',             // Identificador para remover um item
          incrementId: '.ad-advanced-cart-increment',       // Identificador para incrementar o item
          decrementId: '.ad-advanced-cart-decrement',       // Identiticador para decrementar o item
          quantityId: '.ad-advanced-cart-quantity',         // Identificador para o input de quantidade do item
          openAfterAdd: false,                              // Abrir carrinho depois de adicionar
          openOnOver: false,                                // Abrir o carrinho ao possicionar o mouse sobre
          closeTime: 5000,                                  // Timeout para o carrinho fechar
          messageCloseTime: 3000,                           // Timeout para a messagem sumir
          beforeLoad: function(container, data) {},
          afterLoad: function(container, data) { $(container).find("input").setMask(); },
          beforeAdd: function(identifier) {},
          afterAdd: function(data) {},
          validateAdd: function(identifier) { return true; }
        };

    // INITIALIZE
    settings = jQuery.extend({}, settings, options);
    template = Handlebars.compile(jQuery(settings.template).html());
    isOpenedTimeout = Timeout(settings.closeTime);
    messageTimeout = Timeout(settings.messageCloseTime);

    cart = (function() {
      var cart = this;

      // INITIALIZE
      cart.message = "";
      cart.items = [];
      cart.isOpened = false;
      cart.scrollPosition = 360;
      cart.summary = {
        total: 0.0,
        quantity: 0
      };

      function getCart() {
        jQuery.get('/cart_resume.json', function(data) {
          load(data);
        });
      }

      getCart();

      function load(data) {
        var container = jQuery(self);
        settings.beforeLoad.call(self, container, data);
        cart.items = data.items;
        cart.summary = data.summary;
        jQuery(container).html(template(cart));
        jQuery(settings.itemsId).scrollTop(cart.scrollPosition);
        settings.afterLoad.call(self, container, data);
      }

      function reload(data) {
        if (!data.response) {
          cart.message = data.message;
          messageTimeout.set(function() {
            cart.message = "";
            load(cart);
          });
        }
        load(data.resume);
      }

      function findItem(key) {
        var i = 0,
            len = items.length;

        for (; i < len; i++) {
          if (items[i].key === key) {
            return items[i];
          }
        }
        return null;
      }

      function update(item, quantity) {
        jQuery.ajax({
          type: 'POST',
          dataType: 'json',
          url: "/update_cart",
          data: { id: item.key, quantity: quantity, _method: 'PUT' }
        }).done(function(data) {
          reload(data);
        });
      }

      // PUBLIC
      function addItem(identifier, callback) {
        var item = null;
        jQuery.ajax({
          type: 'POST',
          dataType: 'json',
          url: "/purchase",
          data: { cart_item: identifier }
        }).done(function(data) {
          if (callback) {
            callback.call(this, data);
          }
          reload(data);
        });
      }

      function updateQuantity(key, quantity) {
        var item = findItem(key);

        if (typeof quantity == "number" && !isNaN(quantity)) {
          cart.message = "";
          update(item, quantity);
        } else {
          cart.message = "Quantidade invlida";
          load(cart);
          messageTimeout.set(function() {
            cart.message = "";
            load(cart);
          });
         }
      }

      function remove(key) {
        var item = findItem(key);

        jQuery.ajax({
          type: 'DELETE',
          dataType: 'json',
          url: "/remove_from_cart",
          data: { id: item.key }
        }).done(function(data) {
          reload(data);
        });
      }

      function increment(key) {
        var item = findItem(key),
            quantity = item.quantity;

        if (item.uses_decimal) {
          quantity = quantity + 0.1;
        } else {
          quantity = quantity + 1;
        }

        update(item, quantity);
      }

      function decrement(key) {
        var item = findItem(key),
            quantity = item.quantity;

        if (item.uses_decimal) {
          quantity = quantity - 0.1;
        } else {
          quantity = quantity - 1;
        }

        update(item, quantity);
      }

      function isOpened() {
        return cart.isOpened;
      }

      function open() {
        cart.isOpened = true;
      }

      function close() {
        cart.isOpened = false;
      }

      function setScrollPosition(value) {
        cart.scrollPosition = value;
      }

      return {
        addItem: addItem,
        updateQuantity: updateQuantity,
        remove: remove,
        increment: increment,
        decrement: decrement,
        isOpened: isOpened,
        open: open,
        close: close,
        setScrollPosition: setScrollPosition,
        getCart: getCart
      };
    })();

    // MEDIATOR SUBCRIBES
    Mediator.subscribe('advancedCart:addItem', function(identifier) {
      var _self = this;
      if (settings.validateAdd.call(_self, identifier)) {
        settings.beforeAdd.call(_self, identifier);

        cart.addItem(identifier, function(data) {
          if (settings.openAfterAdd && !cart.isOpened()) {
            cart.open();
          }
          if (cart.isOpened()) {
            isOpenedTimeout.set(function() {
              jQuery(settings.containerId).removeClass('ad-open');
              cart.close();
            });
          }
          settings.afterAdd.call(_self, data);
          Mediator.publish('advancedCartBox:addItem', data);
        });
      } else {
        Mediator.publish('advancedCartBox:addInvalidItem', 'O produto adicionado est invlido');
      }
    });

    Mediator.subscribe('advancedCart:getCart', function() {
      cart.getCart();
    });

    // EVENTS
    jQuery(self).on('mouseenter', settings.headerId, function(event) {
      if(settings.openOnOver) {
        event.preventDefault();
        var container = jQuery(settings.containerId);

        jQuery(container).addClass('ad-open');
        cart.open();
        Mediator.publish('advancedCart:open', container);
      }
    });

    jQuery(self).on('click', settings.headerId, function(event) {
      event.preventDefault();
      var container = jQuery(settings.containerId);
      if (cart.isOpened()) {
        jQuery(container).removeClass('ad-open');
        cart.close();
        Mediator.publish('advancedCart:close', container);
      } else {
        jQuery(container).addClass('ad-open');
        cart.open();
        Mediator.publish('advancedCart:open', container);
      }
    });

    jQuery(self).on('mouseenter', settings.containerId, function(event) {
      if (cart.isOpened()) {
        isOpenedTimeout.unset();
      }
      Mediator.publish('advancedCart:mouseenter', jQuery(settings.containerId));
    });

    jQuery(self).on('mouseleave', settings.containerId, function(event) {
      if (cart.isOpened()) {
        isOpenedTimeout.set(function() {
          cart.close();
          jQuery(settings.containerId).removeClass('ad-open');
        });
      }
      Mediator.publish('advancedCart:mouseleave', jQuery(settings.containerId));
    });

    jQuery(self).on('click', settings.incrementId, function(event) {
      event.preventDefault();
      var itemBox = jQuery(this).parents(settings.itemId),
          key = jQuery(itemBox).data('key');

      cart.setScrollPosition($(settings.itemsId).scrollTop());
      jQuery(itemBox).addClass('ad-load');
      cart.increment(key);
      Mediator.publish('advancedCart:increment', key);
    });

    jQuery(self).on('click', settings.decrementId, function(event) {
      event.preventDefault();
      var itemBox = jQuery(this).parents(settings.itemId),
          key = jQuery(itemBox).data('key');

      cart.setScrollPosition($(settings.itemsId).scrollTop());
      jQuery(itemBox).addClass('ad-load');
      cart.decrement(key);
      Mediator.publish('advancedCart:decrement', key);
    });

    jQuery(self).on('change', settings.quantityId, function(event) {
      var itemBox = jQuery(this).parents(settings.itemId),
          quantity = parseFloat(jQuery(this).val()),
          key = jQuery(itemBox).data('key');

      cart.setScrollPosition($(settings.itemsId).scrollTop());
      jQuery(itemBox).addClass('ad-load');
      cart.updateQuantity(key, quantity);
      Mediator.publish('advancedCart:updateQuantity', key, quantity);
    });

    jQuery(self).on('click', settings.removeId, function(event) {
      event.preventDefault();
      var key = jQuery(this).parents(settings.itemId).data('key');

      cart.setScrollPosition($(settings.itemsId).scrollTop());
      cart.remove(key);
      Mediator.publish('advancedCart:remove', key);
    });

    // HENDLEBARS HELPERS
    Handlebars.registerHelper('currency', function(value) {
      return new Handlebars.SafeString(numberToCurrency(value));
    });

    Handlebars.registerHelper('pluralize', function(value, singularWord, pluralWord) {
      return new Handlebars.SafeString(pluralize(value, singularWord, pluralWord));
    });

    Handlebars.registerHelper('decimal', function(value) {
      var decimalValue = parseFloat(value).toFixed(2).replace(".", ",");
      return new Handlebars.SafeString(decimalValue);
    });
  };
})(jQuery, Handlebars);

